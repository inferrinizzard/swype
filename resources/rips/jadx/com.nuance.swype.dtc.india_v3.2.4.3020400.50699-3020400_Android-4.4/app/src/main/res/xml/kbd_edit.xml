<?xml version="1.0" encoding="utf-8"?>
<KeyboardEx xmlns:ns="http://schemas.android.com/apk/res-auto"
    ns:keyWidth="@fraction/default_4_key_width"
    ns:invalidDockModes="split">
    <Row
        ns:rowEdgeFlags="top"
        ns:rowBackground="?attr/firstRowBackground">
        <Key
            ns:codes="@integer/keycode_cut"
            ns:keyEdgeFlags="left"
            ns:keyIcon="?attr/symKeyboardCut"
            ns:keyIconRecolor="?attr/iconRecolorCut"
            ns:popupLabel="@string/label_cut"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_tab"
            ns:iconPreview="?attr/symKeyboardFeedbackTab"
            ns:keyIcon="?attr/symKeyboardTab"
            ns:keyIconRecolor="?attr/iconRecolorTab"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_arrow_up"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackArrowUp"
            ns:keyIcon="?attr/symKeyboardArrowUp"
            ns:keyIconRecolor="?attr/iconRecolorArrowUp"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_mode_back"
            ns:keyEdgeFlags="right"
            ns:keyIcon="?attr/symKeyboardBack"
            ns:keyIconRecolor="?attr/iconRecolorBack"
            ns:popupLabel="@string/label_back"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:codes="@integer/keycode_copy"
            ns:keyEdgeFlags="left"
            ns:keyIcon="?attr/symKeyboardCopy"
            ns:keyIconRecolor="?attr/iconRecolorCopy"
            ns:popupLabel="@string/label_copy"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_arrow_left"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackArrowLeft"
            ns:keyIcon="?attr/symKeyboardArrowLeft"
            ns:keyIconRecolor="?attr/iconRecolorArrowLeft"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_select"
            ns:isSticky="true"
            ns:keyIcon="@drawable/sym_keyboard_select_text_toggle"
            ns:keyIconRecolor="?attr/iconRecolorSelectText"
            ns:popupLabel="@string/label_select_text"
            ns:keyBackground="@drawable/btn_keyboard_key_toggle_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_arrow_right"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackArrowRight"
            ns:keyIcon="?attr/symKeyboardArrowRight"
            ns:keyIconRecolor="?attr/iconRecolorArrowRight"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:codes="@integer/keycode_paste"
            ns:keyEdgeFlags="left"
            ns:keyIcon="?attr/symKeyboardPaste"
            ns:keyIconRecolor="?attr/iconRecolorPaste"
            ns:popupLabel="@string/label_paste"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_select_all"
            ns:keyIcon="?attr/symKeyboardSelectAll"
            ns:keyIconRecolor="?attr/iconRecolorSelectAll"
            ns:popupLabel="@string/label_select_all"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_arrow_down"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackArrowDown"
            ns:keyIcon="?attr/symKeyboardArrowDown"
            ns:keyIconRecolor="?attr/iconRecolorArrowDown"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="false"
            ns:iconPreview="?attr/symKeyboardFeedbackDelete"
            ns:keyIcon="?attr/symKeyboardDelete"
            ns:keyIconRecolor="?attr/iconRecolorDelete"
            ns:altPreviewIcon="?attr/symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row
        ns:rowEdgeFlags="bottom"
        ns:rowBackground="?attr/bottomRowBackground">
        <Key
            ns:codes="@integer/keycode_swype"
            ns:altCode="@integer/keycode_settings"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/keyboardSwypeLogoFeedback"
            ns:keyIcon="?attr/keyboardSwypeLogo"
            ns:keyIconRecolor="?attr/iconRecolorSwypeLogo"
            ns:altPreviewIcon="?attr/keyboardSwypeLogoFeedback"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_abc_layer"
            ns:keyLabel="@string/label_alpha_key"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:codes="@integer/keycode_space"
            ns:iconPreview="?attr/symKeyboardFeedbackSpace"
            ns:keyIcon="?attr/symKeyboardSpaceKey"
            ns:keyIconRecolor="?attr/iconRecolorSpaceKey"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_enter"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_return_feedback"
            ns:keyIcon="@drawable/sym_keyboard_return"
            ns:keyIconRecolor="?attr/iconRecolorReturn"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
</KeyboardEx>
