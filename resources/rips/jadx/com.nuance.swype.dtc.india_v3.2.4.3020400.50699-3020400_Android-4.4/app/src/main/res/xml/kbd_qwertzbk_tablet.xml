            ns:altKeyLabel="("
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_j"
            ns:keyLabel="j"
            ns:altKeyLabel=")"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_k"
            ns:keyLabel="k"
            ns:altKeyLabel=":"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_l"
            ns:keyLabel="l"
            ns:altKeyLabel=";"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x000015b9"
            ns:keyLabel="č"
            ns:altKeyLabel="„"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x000015b8"
            ns:keyLabel="ć"
            ns:altKeyLabel="“"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x000015bc"
            ns:keyLabel="ž"
            ns:altKeyLabel="—"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyWidth="@fraction/return_12_12_key_tablet_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:altTextSize="@dimen/alt_emoji_icon_size_big"
            ns:altTextGravity="right"
            ns:codes="@integer/keycode_enter"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_return_feedback"
            ns:keyIcon="@drawable/sym_keyboard_return"
            ns:keyIconRecolor="?attr/iconRecolorReturn"
            ns:altIcon="?attr/emojiIconKeySecondary"
            ns:altPreviewIcon="?attr/emojiIconPopupKeypress"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_12_8_key_tablet_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="left"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:isLockable="true"
            ns:iconPreview="@drawable/sym_keyboard_shift_cycle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_shift_cycle"
            ns:keyIconRecolor="?attr/iconRecolorShift"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_key_shift_cycle"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_y"
            ns:keyLabel="y"
            ns:altKeyLabel="~"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_x"
            ns:keyLabel="x"
            ns:altKeyLabel="!"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_c"
            ns:keyLabel="c"
            ns:altKeyLabel="#"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_v"
            ns:keyLabel="v"
            ns:altKeyLabel="="/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_b"
            ns:keyLabel="b"
            ns:altKeyLabel="/"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_n"
            ns:keyLabel="n"
            ns:altKeyLabel="+"/>
        <Key
            ns:popupCharacters="@string/alternates_for_qwertzbk_m"
            ns:keyLabel="m"
            ns:altKeyLabel="?"/>
        <Key
            ns:keyLabel="@"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/shift_12_8_key_tablet_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="right"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:isLockable="true"
            ns:iconPreview="@drawable/sym_keyboard_shift_cycle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_shift_cycle"
            ns:keyIconRecolor="?attr/iconRecolorShift"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_key_shift_cycle"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
