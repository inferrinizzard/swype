<?xml version="1.0" encoding="utf-8"?>
<KeyboardEx xmlns:ns="http://schemas.android.com/apk/res-auto" ns:keyWidth="@fraction/default_10_key_width">
    <include ns:src="@xml/kbd_number_row_default"/>
    <Row ns:rowBackground="?attr/firstRowBackground">
        <Key
            ns:keyEdgeFlags="left"
            ns:keyLabel="q"
            ns:altKeyLabel="1"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="w"
            ns:altKeyLabel="2"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="e"
            ns:altKeyLabel="3"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="r"
            ns:altKeyLabel="4"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="t"
            ns:altKeyLabel="5"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="y"
            ns:altKeyLabel="6"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="u"
            ns:altKeyLabel="7"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="i"
            ns:altKeyLabel="8"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyLabel="o"
            ns:altKeyLabel="9"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyEdgeFlags="right"
            ns:keyLabel="p"
            ns:altKeyLabel="0"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:keyEdgeFlags="left"
            ns:keyLabel="a"
            ns:altKeyLabel="@"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="s"
            ns:altKeyLabel="¥"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="d"
            ns:altKeyLabel="&amp;"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="f"
            ns:altKeyLabel="#"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="g"
            ns:altKeyLabel="("
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="h"
            ns:altKeyLabel=")"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="j"
            ns:altKeyLabel=":"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="k"
            ns:altKeyLabel="※"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyLabel="l"
            ns:altKeyLabel="「"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:codes="0x30fc"
            ns:keyEdgeFlags="right"
            ns:altKeyOutputText="」"
            ns:keyLabel="ー"
            ns:altKeyLabel="」"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_7_10_key_width"
            ns:codes="@integer/keycode_segmentation"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/japaneseFeedbackSegmentation"
            ns:keyIcon="?attr/japaneseSegmentation"
            ns:keyIconRecolor="?attr/iconRecolorJapaneseSegmentation"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="z"
            ns:altKeyLabel="*"/>
        <Key
            ns:keyLabel="x"
            ns:altKeyLabel="！"/>
        <Key
            ns:altKeyOutputText="時"
            ns:keyLabel="c"
            ns:altKeyLabel="時"/>
        <Key
            ns:altKeyOutputText="分"
            ns:keyLabel="v"
            ns:altKeyLabel="分"/>
        <Key
            ns:altKeyOutputText="月"
            ns:keyLabel="b"
            ns:altKeyLabel="月"/>
        <Key
            ns:altKeyOutputText="日"
            ns:keyLabel="n"
            ns:altKeyLabel="日"/>
        <Key
            ns:keyLabel="m"
            ns:altKeyLabel="？"/>
        <Key
            ns:keyWidth="@fraction/delete_7_10_key_width"
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackDelete"
            ns:keyIcon="?attr/symKeyboardDelete"
            ns:keyIconRecolor="?attr/iconRecolorDelete"
            ns:altPreviewIcon="?attr/symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row
        ns:rowEdgeFlags="bottom"
        ns:rowBackground="?attr/bottomRowBackground">
        <Key
            ns:keyWidth="@fraction/swype_key_width"
            ns:codes="@integer/keycode_swype"
            ns:altCode="@integer/keycode_settings"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/keyboardSwypeLogoFeedback"
            ns:keyIcon="?attr/keyboardSwypeLogo"
            ns:keyIconRecolor="?attr/iconRecolorSwypeLogo"
            ns:altPreviewIcon="?attr/keyboardSwypeLogoFeedback"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/symbols_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_sym_layer"
            ns:popupKeyboard="@xml/popup_layers"
            ns:keyLabel="@string/label_symbol_key"
            ns:altIcon="?attr/symKeyboardLayers"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:codes="@integer/keycode_language_quick_switch"
            ns:iconPreview="?attr/cjkJapaneseFeedbackToggleJapanese"
            ns:keyIcon="?attr/cjkJapaneseToggleJapanese"
            ns:keyIconRecolor="?attr/iconRecolorJapaneseToggle"
            ns:keyboardSettings="switchable"
            ns:keyGroup="@+id/handwriting_key"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:altTextSize="@dimen/alt_text_size_big"
            ns:popupCharacters="@string/alternates_for_japanesest_comma"
            ns:iconPreview="?attr/japaneseFeedbackComma"
            ns:keyOutputText="、"
            ns:altKeyLabel="~"
            ns:keyIcon="?attr/japaneseComma"
            ns:keyIconRecolor="?attr/iconRecolorJapaneseComma"
            ns:keyboardMode="number|im|normal"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:altTextSize="@dimen/alt_text_size_big"
            ns:popupCharacters="@string/alternates_for_japanesest_comma"
            ns:keyLabel="@"
            ns:altKeyLabel="~"
            ns:keyboardMode="email"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"/>
        <Key
            ns:keyWidth="@fraction/space_key_width_cjk"
            ns:altTextColor="?attr/defaultAltTextColorSpace"
            ns:codes="@integer/keycode_space"
            ns:altCode="@integer/keycode_language_menu"
            ns:keyIcon="?attr/symKeyboardSpace"
            ns:keyIconRecolor="?attr/iconRecolorSpace"
            ns:showPopup="false"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom_space"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:altTextSize="@dimen/alt_text_size_big"
            ns:popupCharacters="@string/alternates_for_japanese_period"
            ns:iconPreview="?attr/chineseFeedbackPeriod"
            ns:keyOutputText="。"
            ns:altKeyLabel="."
            ns:keyIcon="?attr/chinesePeriod"
            ns:keyIconRecolor="?attr/iconRecolorChinesePeriod"
            ns:keyboardMode="number|im|normal"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:popupCharacters="@string/alternates_for_japanese_period"
            ns:keyLabel="."
            ns:keyboardMode="email"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:codes="@integer/keycode_speech"
            ns:altCode="@integer/keycode_speech_app"
            ns:iconPreview="@drawable/sym_keyboard_speech_feedback"
            ns:keyIcon="@drawable/sym_keyboard_speech"
            ns:keyIconRecolor="?attr/iconRecolorSpeech"
            ns:keyboardSettings="speech"
            ns:dependentKey="@integer/keycode_space"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/return_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:altTextSize="@dimen/alt_emoji_icon_size"
            ns:altTextGravity="right"
            ns:codes="@integer/keycode_enter"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_return_feedback"
            ns:keyIcon="@drawable/sym_keyboard_return"
            ns:keyIconRecolor="?attr/iconRecolorReturn"
            ns:altIcon="?attr/emojiIconKeySecondary"
            ns:altPreviewIcon="?attr/emojiIconPopupKeypress"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
</KeyboardEx>
