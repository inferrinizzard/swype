<?xml version="1.0" encoding="utf-8"?>
<KeyboardEx xmlns:ns="http://schemas.android.com/apk/res-auto"
    ns:keyWidth="@fraction/default_chinese_keypad_tablet_width"
    ns:invalidDockModes="split"
    ns:keyBackground="@drawable/btn_keyboard_key_5row">
    <Row ns:rowBackground="?attr/firstRowBackground">
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:keyTextColor="@color/transparent"
            ns:codes="-1"
            ns:keyEdgeFlags="left"
            ns:showPopup="false"
            ns:keyBackground="?attr/transBackground"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyOutputText="1"
            ns:keyLabel="1"
            ns:keyBackground="@drawable/btn_keyboard_key_5row_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="97,98,99"
            ns:popupCharacters="日月金2"
            ns:keyLabel="日月金"
            ns:altKeyLabel="2"
            ns:keyBackground="@drawable/btn_keyboard_key_5row_top"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="100,101,102"
            ns:popupCharacters="木水火3"
            ns:keyLabel="木水火"
            ns:altKeyLabel="3"
            ns:keyBackground="@drawable/btn_keyboard_key_5row_top"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackDelete"
            ns:keyIcon="?attr/symKeyboardDelete"
            ns:keyIconRecolor="?attr/iconRecolorDelete"
            ns:altPreviewIcon="?attr/symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:keyTextColor="@color/transparent"
            ns:codes="0x3f"
            ns:keyEdgeFlags="left"
            ns:showPopup="false"
            ns:keyBackground="?attr/transBackground"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="103,104,105"
            ns:popupCharacters="土竹戈4"
            ns:keyLabel="土竹戈"
            ns:altKeyLabel="4"
            ns:keyBackground="@drawable/btn_keyboard_key_5row_mid"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="106,107,108"
            ns:popupCharacters="十大中5"
            ns:keyLabel="十大中"
            ns:altKeyLabel="5"
            ns:keyBackground="@drawable/btn_keyboard_key_5row_mid"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="109,110,111"
            ns:popupCharacters="一弓人6"
            ns:keyLabel="一弓人"
            ns:altKeyLabel="6"
            ns:keyBackground="@drawable/btn_keyboard_key_5row_mid"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_speech"
            ns:altCode="@integer/keycode_speech_app"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_speech_feedback"
            ns:keyIcon="@drawable/sym_keyboard_speech"
            ns:keyIconRecolor="?attr/iconRecolorSpeech"
            ns:keyboardSettings="speech"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:keyTextColor="@color/transparent"
            ns:codes="-1"
            ns:keyEdgeFlags="left"
            ns:showPopup="false"
            ns:keyBackground="?attr/transBackground"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="112,113,114,115"
            ns:popupCharacters="心手口尸7"
            ns:keyLabel="心手口尸"
            ns:altKeyLabel="7"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="116,117,118"
            ns:popupCharacters="廿山女8"
            ns:keyLabel="廿山女"
            ns:altKeyLabel="8"/>
        <Key
            ns:keyTextSize="@dimen/cangjie_keytext_size"
            ns:keyFontTypeface="non_bold"
            ns:codes="119,120,121"
            ns:popupCharacters="田難卜9"
            ns:keyLabel="田難卜"
            ns:altKeyLabel="9"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyhalf_tablet_width"
            ns:codes="@integer/keycode_swype"
            ns:altCode="@integer/keycode_settings"
            ns:iconPreview="?attr/keyboardSwypeLogoFeedback"
            ns:keyIcon="?attr/keyboardSwypeLogo"
            ns:keyIconRecolor="?attr/iconRecolorSwypeLogo"
            ns:altPreviewIcon="?attr/keyboardSwypeLogoFeedback"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyhalf_tablet_width"
            ns:codes="@integer/keycode_keyboard_resize"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_resize_cycle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_resize_cycle"
            ns:keyIconRecolor="?attr/iconRecolorResize"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row
        ns:rowEdgeFlags="bottom"
        ns:rowBackground="?attr/bottomRowBackground">
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:keyFontTypeface="default_bold"
            ns:codes="@integer/keycode_sym_layer"
            ns:popupKeyboard="@xml/popup_layers"
            ns:keyEdgeFlags="left"
            ns:keyLabel="@string/label_symbol_key"
            ns:altIcon="?attr/symKeyboardLayers"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyhalf_tablet_width"
            ns:codes="@integer/keycode_language_quick_switch"
            ns:iconPreview="?attr/cjkChineseFeedbackToggleChinese"
            ns:keyIcon="?attr/cjkChineseToggleChinese"
            ns:keyIconRecolor="?attr/iconRecolorChineseToggle"
            ns:dependentKey="@integer/keycode_space"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyhalf_tablet_width"
            ns:codes="@integer/keycode_edit"
            ns:iconPreview="?attr/symKeyboardFeedbackKeypad"
            ns:keyIcon="?attr/symKeyboardKeypad"
            ns:keyIconRecolor="?attr/iconRecolorKeypad"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:codes="@integer/keycode_space"
            ns:altCode="@integer/keycode_language_menu"
            ns:isRepeatable="true"
            ns:keyIcon="?attr/symKeyboardSpaceKey"
            ns:keyIconRecolor="?attr/iconRecolorSpaceKey"
            ns:showPopup="false"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:codes="@integer/keycode_multitap_deadkey"
            ns:iconPreview="?attr/symKeyboardFeedbackTimeout"
            ns:altKeyLabel="0"
            ns:keyIcon="?attr/symKeyboardTimeout"
            ns:keyIconRecolor="?attr/iconRecolorTimeout"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/default_chinese_keyside_tablet_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:altTextSize="@dimen/alt_emoji_icon_size_big"
            ns:altTextGravity="right"
            ns:codes="@integer/keycode_enter"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_return_feedback"
            ns:keyIcon="@drawable/sym_keyboard_return"
            ns:keyIconRecolor="?attr/iconRecolorReturn"
            ns:altIcon="?attr/emojiIconKeySecondary"
            ns:altPreviewIcon="?attr/emojiIconPopupKeypress"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_5row"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
</KeyboardEx>
