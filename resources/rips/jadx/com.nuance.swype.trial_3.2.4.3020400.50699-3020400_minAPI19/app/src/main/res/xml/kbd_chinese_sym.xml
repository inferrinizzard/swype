            ns:keyEdgeFlags="left"
            ns:keyLabel="1"
            ns:shiftedKeyLabel="•"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_w"
            ns:keyLabel="2"
            ns:shiftedKeyLabel="™"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
