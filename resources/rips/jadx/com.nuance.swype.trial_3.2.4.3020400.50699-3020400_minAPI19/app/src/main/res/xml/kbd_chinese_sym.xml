            ns:keyLabel="3"
            ns:shiftedKeyLabel="€"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_r"
            ns:keyLabel="4"
            ns:shiftedKeyLabel="‘"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
