        <Key
            ns:popupCharacters="@string/alternates_for_chinese_t"
            ns:keyLabel="5"
            ns:shiftedKeyLabel="^"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_y"
            ns:keyLabel="6"
            ns:shiftedKeyLabel="$"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_u"
            ns:keyLabel="7"
            ns:shiftedKeyLabel="«"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_i"
            ns:keyLabel="8"
            ns:shiftedKeyLabel="»"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_o"
            ns:keyLabel="9"
            ns:shiftedKeyLabel="{"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_p"
            ns:keyEdgeFlags="right"
            ns:keyLabel="0"
            ns:shiftedKeyLabel="}"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_a"
            ns:keyEdgeFlags="left"
            ns:keyLabel="@"
            ns:shiftedKeyLabel="©"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_s"
            ns:keyLabel="¥"
            ns:shiftedKeyLabel="®"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_d"
            ns:keyLabel="&amp;"
            ns:shiftedKeyLabel="|"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_f"
            ns:keyLabel="_"
            ns:shiftedKeyLabel="°"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_g"
            ns:keyLabel="（"
            ns:shiftedKeyLabel="["
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_h"
            ns:keyLabel="）"
            ns:shiftedKeyLabel="]"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_j"
            ns:keyLabel=":"
            ns:shiftedKeyLabel="&lt;"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_k"
            ns:keyLabel=";"
            ns:shiftedKeyLabel="&gt;"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_l"
            ns:keyEdgeFlags="right"
            ns:keyLabel="&quot;"
            ns:shiftedKeyLabel="£"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_7_10_key_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="left"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:iconPreview="@drawable/sym_keyboard_sym_toggle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_sym_toggle"
            ns:keyIconRecolor="?attr/iconRecolorSymbolsToggle"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_z"
            ns:keyLabel="±"
            ns:shiftedKeyLabel="¢"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_x"
            ns:keyLabel="！"
            ns:shiftedKeyLabel="¡"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_c"
            ns:keyLabel="#"
            ns:shiftedKeyLabel="%"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_v"
            ns:keyLabel="="
            ns:shiftedKeyLabel="*"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_chinese_b"
            ns:keyLabel="/"
            ns:shiftedKeyLabel="\\"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
