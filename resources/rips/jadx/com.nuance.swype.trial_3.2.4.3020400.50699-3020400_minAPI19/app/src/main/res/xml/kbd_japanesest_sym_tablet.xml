<?xml version="1.0" encoding="utf-8"?>
<KeyboardEx xmlns:ns="http://schemas.android.com/apk/res-auto"
    ns:keyWidth="@fraction/default_10_key_tablet_width"
    ns:canSwypePopupCharacters="false">
    <Row
        ns:rowEdgeFlags="top"
        ns:rowBackground="?attr/firstRowBackground">
        <Key
            ns:keyWidth="@fraction/delete_key_tablet_width"
            ns:codes="@integer/keycode_edit"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/symKeyboardFeedbackKeypad"
            ns:keyIcon="?attr/symKeyboardKeypad"
            ns:keyIconRecolor="?attr/iconRecolorKeypad"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="1"
            ns:shiftedKeyLabel="&apos;"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="2"
            ns:shiftedKeyLabel="&quot;"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="3"
            ns:shiftedKeyLabel="["
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="4"
            ns:shiftedKeyLabel="]"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="5"
            ns:shiftedKeyLabel="&lt;"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="6"
            ns:shiftedKeyLabel="&gt;"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="7"
            ns:shiftedKeyLabel="«"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="8"
            ns:shiftedKeyLabel="»"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="9"
            ns:shiftedKeyLabel="{"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="0"
            ns:shiftedKeyLabel="}"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/delete_key_tablet_width"
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackDelete"
            ns:keyIcon="?attr/symKeyboardDelete"
            ns:keyIconRecolor="?attr/iconRecolorDelete"
            ns:altPreviewIcon="?attr/symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:keyWidth="@fraction/return_10_10_key_tablet_width"
            ns:codes="@integer/keycode_tab"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/symKeyboardFeedbackTab"
            ns:keyIcon="?attr/symKeyboardTabTablet"
            ns:keyIconRecolor="?attr/iconRecolorTabTablet"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="×"
            ns:shiftedKeyLabel="^"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="¥"
            ns:shiftedKeyLabel="$"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="&amp;"
            ns:shiftedKeyLabel="¢"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="#"
            ns:shiftedKeyLabel="="
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="("
            ns:shiftedKeyLabel="|"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel=")"
            ns:shiftedKeyLabel="+"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel=":"
            ns:shiftedKeyLabel=";"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:shiftedOutputText="〒"
            ns:keyLabel="※"
            ns:shiftedKeyLabel="〒"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="「"
            ns:shiftedKeyLabel="『"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="」"
            ns:shiftedKeyLabel="』"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/return_10_10_key_tablet_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:altTextSize="@dimen/alt_emoji_icon_size_big"
            ns:altTextGravity="right"
            ns:codes="@integer/keycode_enter"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_return_feedback"
            ns:keyIcon="@drawable/sym_keyboard_return"
            ns:keyIconRecolor="?attr/iconRecolorReturn"
            ns:altIcon="?attr/emojiIconKeySecondary"
            ns:altPreviewIcon="?attr/emojiIconPopupKeypress"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_10_8_key_tablet_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="left"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:iconPreview="@drawable/sym_keyboard_sym_toggle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_sym_toggle"
            ns:keyIconRecolor="?attr/iconRecolorSymbolsToggle"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="*"
            ns:shiftedKeyLabel="¤"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="！"
            ns:shiftedKeyLabel="…"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyOutputText="時"
            ns:shiftedOutputText="%"
            ns:keyLabel="時"
            ns:shiftedKeyLabel="%"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyOutputText="分"
            ns:shiftedOutputText="/"
            ns:keyLabel="分"
            ns:shiftedKeyLabel="/"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyOutputText="月"
            ns:shiftedOutputText="\\"
            ns:keyLabel="月"
            ns:shiftedKeyLabel="\\"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyOutputText="日"
            ns:shiftedOutputText="~"
            ns:keyLabel="日"
            ns:shiftedKeyLabel="~"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="？"
            ns:shiftedKeyLabel="々"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyLabel="@"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/shift_10_8_key_tablet_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="right"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:iconPreview="@drawable/sym_keyboard_sym_toggle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_sym_toggle"
            ns:keyIconRecolor="?attr/iconRecolorSymbolsToggle"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <Row
        ns:rowEdgeFlags="bottom"
        ns:rowBackground="?attr/bottomRowBackground">
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_tablet_width"
            ns:codes="@integer/keycode_swype"
            ns:altCode="@integer/keycode_settings"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/keyboardSwypeLogoFeedback"
            ns:keyIcon="?attr/keyboardSwypeLogo"
            ns:keyIconRecolor="?attr/iconRecolorSwypeLogo"
            ns:altPreviewIcon="?attr/keyboardSwypeLogoFeedback"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_tablet_width"
            ns:keyTextSize="@dimen/key_label_text_size_large"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_abc_layer"
            ns:popupKeyboard="@xml/popup_layers"
            ns:keyLabel="@string/label_jp_abc"
            ns:altIcon="?attr/symKeyboardLayers"
            ns:keyType="function"
            ns:shiftTransition="swap"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_punctab_key_tablet_width"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:keyLabel=","
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_punctab_key_tablet_width"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:keyLabel="-"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/space_key_tablet_width_sym"
            ns:altTextColor="?attr/defaultAltTextColorSpace"
            ns:codes="@integer/keycode_space"
            ns:altCode="@integer/keycode_language_menu"
            ns:keyIcon="?attr/symKeyboardSpace"
            ns:keyIconRecolor="?attr/iconRecolorSpace"
            ns:showPopup="false"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom_space"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_punctab_key_tablet_width"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:keyLabel="・"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_punctab_key_tablet_width"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:keyLabel="."
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_tablet_width"
            ns:codes="@integer/keycode_speech"
            ns:altCode="@integer/keycode_speech_app"
            ns:iconPreview="@drawable/sym_keyboard_speech_feedback"
            ns:keyIcon="@drawable/sym_keyboard_speech"
            ns:keyIconRecolor="?attr/iconRecolorSpeech"
            ns:keyboardSettings="speech"
            ns:dependentKey="@integer/keycode_space"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_tablet_width"
            ns:codes="@integer/keycode_keyboard_resize"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_resize_cycle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_resize_cycle"
            ns:keyIconRecolor="?attr/iconRecolorResize"
            ns:keyboardSettings="resizable"
            ns:dependentKey="@integer/keycode_space"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
</KeyboardEx>
