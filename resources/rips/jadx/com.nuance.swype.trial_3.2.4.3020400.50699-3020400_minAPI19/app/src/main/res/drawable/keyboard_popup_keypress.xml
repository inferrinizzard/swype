<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="?attr/popupBackgroundColor"/>
            <stroke
                android:width="?attr/popupBorderThickness"
                android:color="?attr/popupBorderColor"/>
            <corners android:radius="?attr/popupRadius"/>
            <padding
                android:left="?attr/popupBorderPaddingLeft"
                android:top="?attr/popupBorderPaddingTop"
                android:right="?attr/popupBorderPaddingRight"
                android:bottom="?attr/popupBorderPaddingBottom"/>
        </shape>
    </item>
</layer-list>
