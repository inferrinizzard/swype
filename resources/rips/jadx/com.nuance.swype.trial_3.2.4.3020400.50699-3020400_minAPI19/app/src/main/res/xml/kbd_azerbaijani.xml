<?xml version="1.0" encoding="utf-8"?>
<KeyboardEx xmlns:ns="http://schemas.android.com/apk/res-auto"
    ns:keyWidth="@fraction/default_12_key_width"
    ns:keyTextSize="?attr/keyCharacterSizeSmall">
    <include ns:src="@xml/kbd_number_row_default"/>
    <Row ns:rowBackground="?attr/firstRowBackground">
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_q"
            ns:keyEdgeFlags="left"
            ns:keyLabel="q"
            ns:altKeyLabel="*"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e39"
            ns:keyLabel="ü"
            ns:altKeyLabel="w"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_e"
            ns:keyLabel="e"
            ns:altKeyLabel="1"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_r"
            ns:keyLabel="r"
            ns:altKeyLabel="2"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_t"
            ns:keyLabel="t"
            ns:altKeyLabel="3"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_y"
            ns:keyLabel="y"
            ns:altKeyLabel="4"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_u"
            ns:keyLabel="u"
            ns:altKeyLabel="5"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_i"
            ns:keyLabel="i"
            ns:altKeyLabel="6"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_o"
            ns:keyLabel="o"
            ns:altKeyLabel="7"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_p"
            ns:keyLabel="p"
            ns:altKeyLabel="8"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e38"
            ns:keyLabel="ö"
            ns:altKeyLabel="9"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e3a"
            ns:keyEdgeFlags="right"
            ns:keyLabel="ğ"
            ns:altKeyLabel="0"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_a"
            ns:keyLabel="a"
            ns:altKeyLabel="@"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_s"
            ns:keyLabel="s"
            ns:altKeyLabel="$"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_d"
            ns:keyLabel="d"
            ns:altKeyLabel="&amp;"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_f"
            ns:keyLabel="f"
            ns:altKeyLabel="%"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_g"
            ns:keyLabel="g"
            ns:altKeyLabel="_"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_h"
            ns:keyLabel="h"
            ns:altKeyLabel="+"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_j"
            ns:keyLabel="j"
            ns:altKeyLabel="("
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_k"
            ns:keyLabel="k"
            ns:altKeyLabel=")"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_l"
            ns:keyLabel="l"
            ns:altKeyLabel=":"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e3b"
            ns:keyLabel="ı"
            ns:altKeyLabel=";"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e3d"
            ns:keyEdgeFlags="right"
            ns:keyLabel="ə"
            ns:altKeyLabel="&quot;"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_9_12_key_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="left"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:isLockable="true"
            ns:iconPreview="@drawable/sym_keyboard_shift_cycle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_shift_cycle"
            ns:keyIconRecolor="?attr/iconRecolorShift"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_key_shift_cycle"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_z"
            ns:keyLabel="z"
            ns:altKeyLabel="€"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_x"
            ns:keyLabel="x"
            ns:altKeyLabel="!"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_c"
            ns:keyLabel="c"
            ns:altKeyLabel="#"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_v"
            ns:keyLabel="v"
            ns:altKeyLabel="="/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_b"
            ns:keyLabel="b"
            ns:altKeyLabel="/"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_n"
            ns:keyLabel="n"
            ns:altKeyLabel="\\"/>
        <Key
            ns:popupCharacters="@string/alternates_for_azerbaijani_m"
            ns:keyLabel="m"
            ns:altKeyLabel="«"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e37"
            ns:keyLabel="ç"
            ns:altKeyLabel="»"/>
        <Key
            ns:popupCharacters="@string/jadx_deobf_0x00000e3c"
            ns:keyLabel="ş"
            ns:altKeyLabel="?"/>
        <Key
            ns:keyWidth="@fraction/delete_9_12_key_width"
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="false"
            ns:iconPreview="?attr/symKeyboardFeedbackDelete"
            ns:keyIcon="?attr/symKeyboardDelete"
            ns:keyIconRecolor="?attr/iconRecolorDelete"
            ns:altPreviewIcon="?attr/symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
    <include ns:src="@xml/kbd_bottom_row_default"/>
</KeyboardEx>
