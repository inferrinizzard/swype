            ns:altKeyLabel="n"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011f6"
            ns:keyLabel="ㅡ"
            ns:altKeyLabel="m"/>
        <Key
            ns:keyWidth="@fraction/delete_7_10_key_width"
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="true"
            ns:iconPreview="?attr/symKeyboardFeedbackDelete"
            ns:keyIcon="?attr/symKeyboardDelete"
            ns:keyIconRecolor="?attr/iconRecolorDelete"
            ns:altPreviewIcon="?attr/symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/return_key_baseline"/>
    </Row>
    <Row
        ns:rowEdgeFlags="bottom"
        ns:rowBackground="?attr/bottomRowBackground">
        <Key
            ns:keyWidth="@fraction/swype_key_width"
            ns:codes="@integer/keycode_swype"
            ns:altCode="@integer/keycode_settings"
            ns:keyEdgeFlags="left"
            ns:iconPreview="?attr/keyboardSwypeLogoFeedback"
            ns:keyIcon="?attr/keyboardSwypeLogo"
            ns:keyIconRecolor="?attr/iconRecolorSwypeLogo"
            ns:altPreviewIcon="?attr/keyboardSwypeLogoFeedback"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/symbols_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_sym_layer"
            ns:popupKeyboard="@xml/popup_layers"
            ns:keyLabel="@string/label_symbol_key"
            ns:altIcon="?attr/symKeyboardLayers"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:codes="@integer/keycode_language_quick_switch"
            ns:keyLabel="@string/label_korean_eng_key"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/space_key_width_korean_qwerty"
            ns:altTextColor="?attr/defaultAltTextColorSpace"
            ns:keyFontTypeface="non_bold"
            ns:codes="@integer/keycode_space"
            ns:altCode="@integer/keycode_language_menu"
            ns:keyIcon="?attr/symKeyboardSpace"
            ns:keyIconRecolor="?attr/iconRecolorSpace"
            ns:showPopup="false"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom_space"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyWidth="@fraction/bottom_row_default_key_width"
            ns:keyTextColor="?attr/funcBtmTextColor"
            ns:popupCharacters="@string/alternates_for_period_korean"
            ns:keyLabel="."
            ns:altKeyLabel=",!?"
            ns:keyBackground="@drawable/btn_keyboard_key_bottom"/>
        <Key
            ns:keyWidth="@fraction/return_key_width"
            ns:keyTextSize="@dimen/key_label_text_size"
            ns:keyTextColor="?attr/funcKeyTextColor"
            ns:altTextSize="@dimen/alt_emoji_icon_size"
            ns:altTextGravity="right"
            ns:codes="@integer/keycode_enter"
            ns:keyEdgeFlags="right"
            ns:iconPreview="@drawable/sym_keyboard_return_feedback"
            ns:keyIcon="@drawable/sym_keyboard_return"
            ns:keyIconRecolor="?attr/iconRecolorReturn"
            ns:altIcon="?attr/emojiIconKeySecondary"
            ns:altPreviewIcon="?attr/emojiIconPopupKeypress"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key_bottom"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
    </Row>
</KeyboardEx>
