            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011ef"
            ns:keyLabel="ㅓ"
            ns:altKeyLabel="j"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011ec"
            ns:keyLabel="ㅏ"
            ns:altKeyLabel="k"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011f7"
            ns:keyEdgeFlags="right"
            ns:keyLabel="ㅣ"
            ns:altKeyLabel="l"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
    </Row>
    <Row ns:rowBackground="?attr/thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_7_10_key_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="left"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:iconPreview="@drawable/sym_keyboard_shift_cycle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_shift_cycle"
            ns:keyIconRecolor="?attr/iconRecolorShift"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_key_shift_cycle"
            ns:keyContentBaseline="@dimen/key_content_no_baseline"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011e8"
            ns:keyLabel="ㅋ"
            ns:altKeyLabel="z"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011e9"
            ns:keyLabel="ㅌ"
            ns:altKeyLabel="x"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011e7"
            ns:keyLabel="ㅊ"
            ns:altKeyLabel="c"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011ea"
            ns:keyLabel="ㅍ"
            ns:altKeyLabel="v"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011f5"
