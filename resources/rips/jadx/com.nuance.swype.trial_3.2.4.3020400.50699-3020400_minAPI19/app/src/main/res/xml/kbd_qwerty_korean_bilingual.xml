            ns:keyLabel="ㅂ"
            ns:altKeyLabel="q"
            ns:shiftedKeyLabel="ㅃ"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:shiftCode="ㅉ,W"
            ns:popupCharacters="@string/jadx_deobf_0x000011e6"
            ns:keyLabel="ㅈ"
            ns:altKeyLabel="w"
            ns:shiftedKeyLabel="ㅉ"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:shiftCode="ㄸ,E"
            ns:popupCharacters="@string/jadx_deobf_0x000011e0"
            ns:keyLabel="ㄷ"
            ns:altKeyLabel="e"
            ns:shiftedKeyLabel="ㄸ"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
