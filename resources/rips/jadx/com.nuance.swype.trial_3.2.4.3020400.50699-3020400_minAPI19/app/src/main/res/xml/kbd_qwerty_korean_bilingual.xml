            ns:keyLabel="ㅅ"
            ns:altKeyLabel="t"
            ns:shiftedKeyLabel="ㅆ"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011f3"
            ns:keyLabel="ㅛ"
            ns:altKeyLabel="y"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011f1"
            ns:keyLabel="ㅕ"
            ns:altKeyLabel="u"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011ee"
            ns:keyLabel="ㅑ"
            ns:altKeyLabel="i"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:shiftCode="ㅒ,O"
            ns:popupCharacters="@string/jadx_deobf_0x000011ed"
