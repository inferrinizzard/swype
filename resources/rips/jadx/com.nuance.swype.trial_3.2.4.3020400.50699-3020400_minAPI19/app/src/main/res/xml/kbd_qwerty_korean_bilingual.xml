            ns:altKeyLabel="o"
            ns:shiftedKeyLabel="ㅒ"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:shiftCode="ㅖ,P"
            ns:popupCharacters="@string/jadx_deobf_0x000011f0"
            ns:keyEdgeFlags="right"
            ns:keyLabel="ㅔ"
            ns:altKeyLabel="p"
            ns:shiftedKeyLabel="ㅖ"
            ns:keyBackground="@drawable/btn_keyboard_key_top"/>
    </Row>
    <Row ns:rowBackground="?attr/secondRowBackground">
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011e2"
            ns:keyEdgeFlags="left"
            ns:keyLabel="ㅁ"
            ns:altKeyLabel="a"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011df"
            ns:keyLabel="ㄴ"
            ns:altKeyLabel="s"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011e5"
            ns:keyLabel="ㅇ"
            ns:altKeyLabel="d"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011e1"
            ns:keyLabel="ㄹ"
            ns:altKeyLabel="f"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011eb"
            ns:keyLabel="ㅎ"
            ns:altKeyLabel="g"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
        <Key
            ns:keyFontTypeface="non_bold"
            ns:popupCharacters="@string/jadx_deobf_0x000011f2"
            ns:keyLabel="ㅗ"
            ns:altKeyLabel="h"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"/>
